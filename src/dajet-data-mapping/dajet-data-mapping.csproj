<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>DaJet.Data.Mapping</RootNamespace>
    <AssemblyName>DaJet.Data.Mapping</AssemblyName>
    <Authors>Zhichkin</Authors>
    <Copyright>Zhichkin Â© 2021</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>Library to work with 1C:Enterprise 8 application data directly using C#.
Microsoft SQL Server supported only.</Description>
    <PackageReleaseNotes>Added mapping and serialization:
1. Information register (independent, non-periodic).
2. Information register (recorder, non-periodic).
3. Information register (recorder, periodic).
4. Information register (recorder, periodic by recorder date and time).
5. Balance register.
6. Turnovers register.
7. Bug fixes</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/zhichkin/dajet-data-mapping</PackageProjectUrl>
    <RepositoryUrl>https://github.com/zhichkin/dajet-data-mapping</RepositoryUrl>
    <Version>3.2.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DaJet.Metadata" Version="3.1.7" />
    <PackageReference Include="Microsoft.IO.RecyclableMemoryStream" Version="2.2.0" />
    <PackageReference Include="RabbitMQ.Client" Version="6.2.2" />
  </ItemGroup>

</Project>
